<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>JEU_Rejected</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    TEXT(Refer_to_JEU__c) = &#39;Rejected&#39;,
    ISBLANK( JEU_Reject_Reason__c)
)</errorConditionFormula>
    <errorMessage>JEU Rejection Reason is required</errorMessage>
</ValidationRule>
