<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PYfilled__c</fullName>
    <description>This field is hidden from page layout and is for HR vacancy report. See Help Text</description>
    <externalId>false</externalId>
    <formula>IF((TEXT(Status__c) = &quot;Filled&quot; || TEXT(Status__c) = &quot;Temporarily Vacant&quot; ),PY__c,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This field takes numeric value for a filled position PY.</inlineHelpText>
    <label>PYfilled</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
