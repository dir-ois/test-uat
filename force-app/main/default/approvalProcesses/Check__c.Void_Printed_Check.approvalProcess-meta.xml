    <approvalPageFields>
        <field>Name</field>
        <field>Approved_By__c</field>
        <field>Amount__c</field>
        <field>Status__c</field>
        <field>Case__c</field>
        <field>Check_Batch__c</field>
        <field>Void_Request__c</field>
        <field>Void_Reason__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>Uncheck_Void_Request</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>Update_Status_to_Voided</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <label>Approve Void of Printed Checks</label>
        <name>Approve_Void_of_Printed_Checks</name>
        <rejectionActions>
            <action>
                <name>Uncheck_Void_Request</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <description>This is used to approve one off check voids that have already been printed.  It is started by Process Builder through the Void Request and Void Reason fields on the check object.</description>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>Check__c.Status__c</field>
            <operation>equals</operation>
            <value>Printed</value>
        </criteriaItems>
        <criteriaItems>
            <field>Check__c.Void_Request__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Check__c.Void_Reason__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Void_Requst_Denied</name>
            <type>Alert</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Submitters_Email</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>Void Printed Check</label>
    <nextAutomatedApprover>
        <useApproverFieldOfRecordOwner>false</useApproverFieldOfRecordOwner>
        <userHierarchyField>Manager</userHierarchyField>
    </nextAutomatedApprover>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
