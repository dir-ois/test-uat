   || TEXT ({!myVariable_current.Form1_Status_Type__c}) = &quot;Packet to HR - Part 2 Under Review&quot;
   || TEXT ({!myVariable_current.Form1_Status_Type__c}) = &quot;HR Final Packet - Analyst Approved&quot;
   || TEXT ({!myVariable_current.Form1_Status_Type__c}) = &quot;HR Final Packet - Manager Approved&quot;
