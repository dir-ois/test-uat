    <description>HR Update Form 1 w Latest F1 Status (Using F1 Status Flag)</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>ISNEW()
&amp;&amp;
[Form_1_Status__c].Date_of_Status__c &gt;=  [Form_1_Status__c].Form_1__c.Max_F1_Status_Date__c 
&amp;&amp; 
( 
      TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;Canceled by Division&quot;
   || TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;Canceled by HR&quot;
   || TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;Request to Hire Submitted&quot;
   || TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;Release of Applications&quot;
   || TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;Interviews - Conducted&quot;
   || TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;Packet to HR - Part 1 Received&quot; 
   || TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;Packet to HR - Part 1 Under Review&quot;
   || TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;Packet to HR - Part 2 Received&quot;
   || TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;Packet to HR - Part 2 Under Review&quot;
   || TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;HR Final Packet - Analyst Approved&quot;
   || TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;HR Final Packet - Manager Approved&quot;
   || TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;HR Final Packet - Submitted to CalHR&quot;
   || TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;CalHR - Approved&quot;
   || TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;CalHR - Denied&quot;
   || TEXT ([Form_1_Status__c].Form1_Status_Type__c) = &quot;HR - Job Offer Clearance Notification&quot;
   || CONTAINS( TEXT ([Form_1_Status__c].Form1_Status_Type__c), &quot;Hold&quot;)

)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>ISNEW()
&amp;&amp;
{!myVariable_current.Date_of_Status__c} &gt;=  {!myVariable_current.Form_1__r.Max_F1_Status_Date__c} 
